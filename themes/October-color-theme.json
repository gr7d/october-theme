{
  "name": "October",
  "colors": {
    "editor.background": "#16161d",
    "editor.foreground": "#d0d0de",
    "sideBarTitle.foreground": "#d0d0de",
    "list.warningForeground": "#d0d0de",
    "activityBarBadge.background": "#d0d0de",
    "activityBarBadge.foreground": "#111",
    "activityBar.background": "#16161d",
    "activityBar.border": "#595a63",
    "sideBar.border": "#595a63",
    "sideBar.background": "#16161d",
    "sideBarSectionHeader.border": "#595a63",
    "sideBarSectionHeader.background": "#16161d",
    "titleBar.inactiveBackground": "#16161d",
    "titleBar.activeBackground": "#16161d",
    "gitDecoration.modifiedResourceForeground": "#7eaee4",
    "gitDecoration.untrackedResourceForeground": "#73cd86",
    "gitDecoration.deletedResourceForeground": "#c66b6b",
    "editorGutter.addedBackground": "#5c785f",
    "diffEditor.insertedLineBackground": "#2b3e2d",
    "diffEditor.insertedTextBackground": "#375639",
    "editorGutter.deletedBackground": "#c66b6b",
    "diffEditor.removedLineBackground": "#592d2d",
    "diffEditor.removedTextBackground": "#8d4444",
    "editorGutter.modifiedBackground": "#94a7bd",
    "tab.inactiveBackground": "#16161d",
    "tab.activeBackground": "#111",
    "focusBorder": "#d0d0de",
    "list.activeSelectionBackground": "#111",
    "list.inactiveSelectionBackground": "#111",
    "statusBar.background": "#111",
    "list.hoverBackground": "#111",
    "input.background": "#111",
    "input.placeholderForeground": "#bab6b6",
    "input.foreground": "#d0d0de",
    "inputOption.activeBorder": "#00000000",
    "inputOption.activeBackground": "#414170",
    "editorWidget.background": "#16161d",
    "editorWidget.border": "#58586c",
    "editorSuggestWidget.selectedBackground": "#2f2f3b",
    "quickInput.background": "#16161d",
    "quickInputList.focusBackground": "#2f2f3b",
    "button.background": "#d0d0de",
    "button.foreground": "#111",
    "textLink.foreground": "#7f7fc8",
    "textLink.activeForeground": "#7f7fc8",
    "editorWarning.foreground": "#c9a88b",
    "editorInlayHint.background": "#333",
    "editorInlayHint.foreground": "#d0d0de",
    "badge.background": "#d0d0de",
    "badge.foreground": "#111"
  },
  "tokenColors": [
    {
      "name": "Base",
      "scope": [
        /* let x = `${test}` */ "string.template meta.template.expression",
        /* type T = X<unknown> */ "meta.type punctuation"
      ],
      "settings": {
        "foreground": "#d0d0de"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#5b5e6b"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function.definition"],
      "settings": {
        "foreground": "#fadc83"
      }
    },
    {
      "name": "Variables, JSON keys, other keyword definitions",
      "scope": [
        "variable",
        "string.json.comments",
        "support.type.property-name.json",
        "keyword.other.definition"
      ],
      "settings": {
        "foreground": "#fca664"
      }
    },
    {
      "name": "Object key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#e5c2a8"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant"],
      "settings": {
        "foreground": "#f75d49"
      }
    },
    {
      "name": "Classes, namespaces",
      "scope": ["entity.name.type.class", "entity.name.namespace"],
      "settings": {
        "foreground": "#b5b4ff"
      }
    },
    {
      "name": "Types",
      "scope": [
        "meta.type",
        "entity.name.type",
        "support.type",
        "meta.interface entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e3958c"
      }
    },
    {
      "name": "Strings, headings",
      "scope": ["string.quoted", "string.template", "string.regexp", "heading"],
      "settings": {
        "foreground": "#92df81"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute"],
      "settings": {
        "foreground": "#80dfb1"
      }
    },
    {
      "name": "Links",
      "scope": ["meta.link"],
      "settings": {
        "foreground": "#7f7fc8"
      }
    },
    {
      "name": "Markup code/inline",
      "scope": ["markup.fenced_code", "markup.inline"],
      "settings": {
        "foreground": "#909094"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#fbbd8e"
      }
    },
    {
      "name": "Tag attributes",
      "scope": ["meta.tag.attributes"],
      "settings": {
        "foreground": "#cb9e7b"
      }
    },
    {
      "name": "Storage types",
      "scope": ["storage"],
      "settings": {
        "foreground": "#fa8383"
      }
    },
    {
      "name": "Keyword control, other keywords",
      "scope": ["keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#fb97fa"
      }
    },
    {
      "name": "Keyword op",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#f2c4f1"
      }
    },
    {
      "name": "text",
      "scope": ["text"],
      "settings": {
        "foreground": "#c0c3cd"
      }
    }
  ]
}
